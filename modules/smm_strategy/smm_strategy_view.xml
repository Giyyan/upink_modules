                                icon="terp-gtk-go-back-rtl"
                                />

                        <!-- sending_additional_sentence -> closed -->
                        <button name="action_closed"
                                attrs="{'invisible': ['|', ('check_r', '=', False), ('state', '!=', 'sending_additional_sentence')]}"
                                string="Работы закончены"
                                icon="gtk-apply"
                                />

                    </group>

                </form>
            </field>
