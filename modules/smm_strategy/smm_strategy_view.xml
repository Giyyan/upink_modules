                                string="Работа по продвижению"
                                icon="terp-gtk-go-back-rtl"
                                />

                        <group attrs="{'invisible': ['&amp;', ('targeted_advertising', '!=', True), ('contest', '!=', True)]}">
                            <!-- start_targeting_reclam -> work_on_promotion -->
